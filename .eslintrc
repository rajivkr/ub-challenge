{
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "comma-dangle": 0,
    "no-plusplus": 0,
    "arrow-body-style": 0,
    "one-var-declaration-per-line": 0,
    "no-param-reassign": 0,
    "implicit-arrow-linebreak": 0,
    "object-curly-newline": 0,
    "operator-linebreak": 0,
    "eol-last": 0,
    "guard-for-in": 0,
    "one-var": 0,
    "space-in-parens": 0,
    "no-continue": 0,
    "no-console": 0
  }
}
